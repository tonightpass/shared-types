{
  "private": true,
  "version": "0.2.0",
  "name": "@tonightpass/shared-types",
  "description": "Tonight Pass shared types",
  "author": "Tonight Pass <developers@tonightpass.com>",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://tonightpass.com",
  "repository": "https://github.com/tonightpass/shared-types.git",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "rollup-plugin-copy-sync": "^1.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {},
  "size-limit": [
    {
      "path": "dist/shared-types.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/shared-types.esm.js",
      "limit": "10 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  }
}
